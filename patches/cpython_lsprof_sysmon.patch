 PyObject* pystart_callback(ProfilerObject* self, PyObject *const *args, Py_ssize_t size)
 {
     PyObject* code = args[0];
+
+    /* Debug: log Python function starts seen by cProfile via sys.monitoring */
+    if (PyCode_Check(code)) {
+        PyCodeObject *co = (PyCodeObject *)code;
+
+        const char *funcname = PyUnicode_AsUTF8(co->co_name);
+        const char *filename = PyUnicode_AsUTF8(co->co_filename);
+        int lineno = co->co_firstlineno;
+
+        fprintf(stderr,
+                "CPROFILE PY_START: %s:%d -> %s\n",
+                filename ? filename : "<unknown>",
+                lineno,
+                funcname ? funcname : "<unknown>");
+    }
+
+    /* existing profiling behaviour – don’t remove this */
     ptrace_enter_call((PyObject*)self, (void *)code, (PyObject *)code);
 
     Py_RETURN_NONE;
 }
 
